<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="zeon.com.chatapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Background">
            <intent-filter tools:ignore="ExtraText">
                <action android:name="android.intent.action.MAIN" />
                android:configChanges="orientation|screenSize"
                -
                (BOOL)shouldAutorotateToInterfaceOrientation:(UIInterfaceOrientation)interfaceOrientation;
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Chat_Page" />
        <activity android:name=".Register_Page" />
        <activity android:name=".Chat_Msg_Row" />
        <activity android:name=".SocketInfo" />
        <activity android:name=".Setting" />
        <activity android:name=".back1" />
        <activity android:name=".back2" />
        <activity android:name=".back3" />
        <activity android:name=".back4" />
        <activity android:name=".Main_Chats_Page"></activity>
    </application>

</manifest>