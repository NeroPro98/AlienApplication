<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zeon.com.chatapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Background">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                android:configChanges="orientation|screenSize"
                -
                (BOOL)shouldAutorotateToInterfaceOrientation:(UIInterfaceOrientation)interfaceOrientation;
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Chat_Page" />
        <activity android:name=".Register_Page" />
        <activity android:name=".Chat_Msg_Row" />
        <activity
            android:name=".User_Service"
            android:label="@string/title_activity_user__service"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>